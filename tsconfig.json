{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "rootDir": "./",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "**/node_modules/*",
        ".vscode-test",
        "src/appcenter/lib"
    ]
}